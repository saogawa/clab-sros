name: sros 
prefix: "clab"

topology:
  nodes:

    sr1:
      kind: nokia_sros
      image: vrnetlab/vr-sros:24.7.R1
      type: sr-1
      license: SR_OS_VSR-SIM_license.txt
      startup-config: clab-sros/sr1/tftpboot/config.txt

    sr2:
      kind: nokia_sros
      image: vrnetlab/vr-sros:24.7.R1
      type: sr-1
      license: SR_OS_VSR-SIM_license.txt
      startup-config: clab-sros/sr2/tftpboot/config.txt

  links:
    - endpoints: ["sr1:eth1", "sr2:eth1"]
